<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>ReGrowth: Extinct Animals</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesInsectoids"]/ingredients/li/filter/thingDefs</xpath>
					<value>
						<li>Corpse_RG_ArcticDrak</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesInsectoids"]/fixedIngredientFilter/thingDefs</xpath>
					<value>
						<li>Corpse_RG_ArcticDrak</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>