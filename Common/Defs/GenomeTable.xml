<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RecipeDef>
		<defName>GR_ExtractGenesEquine</defName>
		<label>extract genes from an equine</label>
		<description>Extract genetic material from an equine corpse.</description>
		<jobString>Extracting Genetic Material</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>2500</workAmount>
		<workSkill>Intellectual</workSkill>
		<workSpeedStat>ResearchSpeed</workSpeedStat>
		<researchPrerequisite>GR_ExtractTier2</researchPrerequisite>
		<recipeUsers>
			<li>GR_GeneticExtractionTable</li>	  
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
				
					<thingDefs>
						<li>Corpse_Horse</li>
						<li>Corpse_Donkey</li>
						<li>Corpse_Dromedary</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
		
			<thingDefs>
				<li>Corpse_Horse</li>
				<li>Corpse_Donkey</li>
				<li>Corpse_Dromedary</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<GR_EquineGenetic>1</GR_EquineGenetic>
		</products>
		<skillRequirements>
			<Intellectual>8</Intellectual>
		</skillRequirements>
	</RecipeDef>
	
